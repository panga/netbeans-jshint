<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="Editors">
        <folder name="text">
            <folder name="javascript"/>
        </folder>
        <folder name="AnnotationTypes">
            <file name="jshint-annotation.xml" url="jshint-annotation.xml"/>
        </folder>
    </folder>
    <folder name="TaskList">
        <folder name="Groups">
            <file name="jshint.instance">
                <attr name="instanceCreate" methodvalue="org.netbeans.spi.tasklist.Task.createGroup"/>
                <attr name="localizingBundle" stringvalue="netbeans.jshint.Bundle"/>
                <attr name="groupName" stringvalue="jshint-tasklist"/>
                <attr name="diplayNameKey" stringvalue="LBL_group"/>
                <attr name="descriptionKey" stringvalue="HINT_group"/>
                <attr name="iconKey" stringvalue="ICON_group"/>
            </file>
        </folder>
        <folder name="Scanners">
            <file name="JSHintTaskScanner.instance">
                <attr name="instanceOf" stringvalue="org.netbeans.spi.tasklist.FileTaskScanner"/>
                <attr name="instanceCreate" methodvalue="netbeans.jshint.JSHintTaskScanner.create"/>
            </file>
        </folder>
    </folder>
</filesystem>
